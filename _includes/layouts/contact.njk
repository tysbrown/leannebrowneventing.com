---
layout: layouts/base.njk
section: contact
---
<style>
    #map {
        height: 400px;
        width: 100%;
    }
</style>
<header class="pageparallax">
    <div class="container page-header">
        <div class="row align-items-center">
            <div class="col-12 text-center">
                <h1 class="responsive-header greatvibes hero-header white-text wow fadeIn" data-wow-duration="5s" style="line-height:600px;">Contact <span style="color: #4caf50">Me</span></h1>
            </div>
        </div>
    </div>
</header>
<div class="container my-3">
    <div class="row">
        <div class="col-lg-12">
            <a href="/" class="black-text" style="text-decoration: underline;">Home</a> > Contact
        </div>
    </div>
</div>
<div class="container-fluid p-2">
    <div class="row">
        <div class="col-lg-12 p-0 z-depth-3 white rounded">
            <div id="map"></div>
            <script>
                function initMap() {
                    var lbe = {lat: 28.589, lng: -81.695};
                    var map = new google.maps.Map(
                        document.getElementById('map'), {zoom: 9, center: lbe});
                    var marker = new google.maps.Marker({position: lbe, map: map});
                }
            </script>
            <script defer
                    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA0C8RKsCLaGva8qRuqwk8tmP1mBaqweXQ&callback=initMap">
            </script>
        </div>
    </div>
</div>
<!-- CONTACT FORM -->
<section class="container-fluid z-depth-3 white rounded">
    <div class="row">
        <div class="col-lg-4 p-3 green darken-4 white-text ml-2">
            {{ layoutContent | safe }}
        </div>
        <div class="col-lg-8 p-3 mr-2">
            <form id="contactform" name="contactform" method="POST" data-netlify="true">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="md-form">
                            <input type="text" id="firstname" name="FirstName" class="form-control">
                            <label for="firstname">First Name</label>
                            <small class="firstnamestatus red-text"></small>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="md-form">
                            <input type="text" id="lastname" name="LastName" class="form-control">
                            <label for="lastname">Last Name</label>
                            <small class="lastnamestatus red-text"></small>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6">
                        <div class="md-form">
                            <input type="text" id="email" name="Email" class="form-control">
                            <label for="email">Email Address</label>
                            <small id="emailmessage" class="form-text text-muted">We'll never share your email with anyone else.</small>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="md-form">
                            <input type="text" id="email2" name="Email2" class="form-control">
                            <label for="email2">Re-Enter Email Address</label>
                            <small class="nomatch emailstatus red-text"></small>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="md-form">
                            <input type="text" id="subject" name="Subject" class="form-control">
                            <label for="subject">Subject</label>
                            <small class="subjectstatus red-text"></small>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="md-form">
                            <textarea type="text" id="message" name="Message" rows="10" class="form-control md-textarea"></textarea>
                            <label for="message">Message</label>
                            <small class="messagestatus red-text"></small>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <button type="submit" class="btn btn-green" onclick="validateForm();">Send</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</section>
<!-- CONTACT FORM END -->




