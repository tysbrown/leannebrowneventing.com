{% set css %}
{% include "assets/css/inline.css" %}
{% include "assets/css/bootstrap.min.css" %}
{% include "assets/css/mdb.min.css" %}
{% include "assets/css/style.css" %}
{% endset %}

<style>{{ css | cssmin | safe }}</style>

<!--Navbar-->
<nav class="navbar navbar-expand-lg navbar-dark fixed-top green darken-4">
  <div class="container">
    <a class="navbar-brand py-5" href="index.html" style="font-family: 'Great Vibes';">LBE</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#basicExampleNav"
            aria-controls="basicExampleNav" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <!-- Collapsible content -->
    <div class="collapse navbar-collapse" id="basicExampleNav">
      <!-- Links -->
      {% set navPages = collections.all | eleventyNavigation %}
      <ul class="navbar-nav ml-auto">
        {%- for entry in navPages %}
        <li class="nav-tem ml-3">
          <a href="{{ entry.url | url }}"{% if entry.url == page.url %} data-current="current item" class="nav-link montserrat small font-weight-bold"{% endif %}>{{ entry.title }}</a>
        </li>
        {%- endfor %}
      </ul>
      <!-- Links -->
    </div>
    <!-- Collapsible content -->
  </div>
</nav>
<!--/.Navbar-->