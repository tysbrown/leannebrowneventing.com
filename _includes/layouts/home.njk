<!-- CONTACT FORM -->
<section class="transbg">
<div class="container p-xl-5 p-lg-5 p-md-2 p-sm-2 p-2  mt-xl-0 mt-lg-0 mt-md-5 mt-sm-5 mt-5">
    <div class="row">
        <div class="col-md-12">
            <h1 class="h1-responsive text-center font-weight-bolder greatvibes white-text" style="font-size:5rem;margin:100px 0 100px 0;">Get in touch</h1>
        </div>
    </div>
    <div class="row py-5">
        <div class="col-md-8 wow fadeInLeft">
            <form id="contactform" name="contactform" method="POST" data-netlify="true">
                <div class="row">
                    <div class="col-md-6">
                        <div class="md-form">
                            <input type="text" id="firstname" name="FirstName" class="form-control">
                            <label for="firstname" class="white-text">First Name</label>
                            <small class="firstnamestatus red-text"></small>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="md-form">
                            <input type="text" id="lastname" name="LastName" class="form-control">
                            <label for="lastname" class="white-text">Last Name</label>
                            <small class="lastnamestatus red-text"></small>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <div class="md-form">
                            <input type="text" id="email" name="Email" class="form-control">
                            <label for="email" class="white-text">Email Address</label>
                            <small id="emailmessage" class="form-text text-muted white-text">We'll never share your email with anyone else.</small>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="md-form">
                            <input type="text" id="email2" name="Email2" class="form-control">
                            <label for="email2" class="white-text">Re-Enter Email Address</label>
                            <small class="nomatch emailstatus red-text"></small>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="md-form">
                            <input type="text" id="subject" name="Subject" class="form-control">
                            <label for="subject" class="white-text">Subject</label>
                            <small class="subjectstatus red-text"></small>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="md-form">
                            <textarea type="text" id="message" name="Message" rows="10" class="form-control md-textarea"></textarea>
                            <label for="message" class="white-text">Message</label>
                            <small class="messagestatus red-text"></small>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <button type="submit" class="btn btn-green" onclick="validateForm();">Send</button>
                    </div>
                </div>
            </form>

        </div>
        <div class="col-md-4 green darken-4 pt-3 mt-xl-0 mt-lg-0 mt-md-5 mt-sm-5 mt-5 wow fadeInRight">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <i class="fas fa-4x fa-envelope text-white"></i>
                        <hr class="my-4 white">
                        <p class="font-italic text-white" style="font-size:35px;">
                            Are you in need of an experienced trainer? I am currently accepting new clients in the greater Orlando area.
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</section>
<!-- CONTACT FORM END -->
