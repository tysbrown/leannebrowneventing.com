---
layout: layouts/base.njk
section: home
---
<!-- Parallax header -->
<header class="parallax bg">
    <div class="container h-100">
        <div class="row align-items-center" style="height: 90vh;">
            <div class="col-12 text-center">
                <h1 class="responsive-header hero-header wow fadeIn" data-wow-duration="5s">Leanne Brown <span style="color: #4caf50">Eventing</span></h1>
                <h5 class="responsive-subheader lead text-white montserrat font-weight-bold my-3 wow fadeInUp">EQUESTRIAN TRAINER AND COACH</h5>
            </div>
        </div>
        <div class="row align-items-center" style="height: 10vh;">
            <div class="col-12 text-center text-white">
                <a id="TOP" href="#TOP"><i class="fas fa-4x fa-arrow-alt-circle-down green-text wow bounce" data-wow-iteration="5"></i></a>
            </div>
        </div>
    </div>
</header>
<!-- Parallax header end -->
<!-- ABOUT ME -->
<section class="container p-xl-5 p-lg-5 p-md-2 p-sm-2 p-2">
    <div class="row">
        <div class="col-md-12">
            <h1 class="h1-responsive text-center font-weight-bolder greatvibes mt-xl-0 mt-lg-0 mt-md-5 mt-sm-5 mt-5" style="font-size: 5rem;margin-bottom:100px;">Welcome</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-md-6 wow fadeInLeft">
            <img src="{{ image1 }}" width="100%" class="z-depth-5">
        </div>
        <div class="col-md-6 d-flex align-items-center mt-xl-0 mt-lg-0 mt-md-5 mt-sm-5 mt-5 wow fadeInRight">
            <div class="container">
                <div class="row">
                    <div class="col-md-12 m-3">
                        <h1 class="h1-responsive font-weight-bold green-text">About Me</h1>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 m-3">
                        {{ layoutContent | safe }}
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 m-3">
                        <button type="button" class="btn btn-dark-green" href="about.html">More Info</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- ABOUT ME END -->
<!-- SERVICES -->
