<style>
    /* Set the size of the div element that contains the map */
    #map {
        height: 400px;  /* The height is 400 pixels */
        width: 100%;  /* The width is the width of the web page */
    }
</style>

<header class="pageparallax">
    <div class="container page-header">
        <div class="row align-items-center">
            <div class="col-12 text-center">
                <h1 class="responsive-header greatvibes hero-header white-text wow fadeIn" data-wow-duration="5s" style="line-height:600px;">About <span style="color: #4caf50">Me</span></h1>
            </div>
        </div>
    </div>
</header>
<div class="container my-3">
    <div class="row">
        <div class="col-lg-12">
            <a href="/" class="black-text" style="text-decoration: underline;">Home</a> > About
        </div>
    </div>
</div>
<div class="container my-3">
    <div class="row">
        <div class="col-lg-12 mx-xl-0 mx-lg-0 mx-md-3 mx-sm-3 mx-3 white rounded z-depth-3">
            <div id="map"></div>
            <script>
                // Initialize and add the map
                function initMap() {
                    // The location of Uluru
                    var lbe = {lat: 28.589, lng: -81.695};
                    // The map, centered at Uluru
                    var map = new google.maps.Map(
                        document.getElementById('map'), {zoom: 4, center: lbe});
                    // The marker, positioned at Uluru
                    var marker = new google.maps.Marker({position: lbe, map: map});
                }
            </script>
            <script defer
                    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA0C8RKsCLaGva8qRuqwk8tmP1mBaqweXQ&callback=initMap">
            </script>
        </div>
    </div>
</div>
<div class="container">
    <div class="row">
        <div class="col-lg-12 p-3 mx-xl-0 mx-lg-0 mx-md-3 mx-sm-3 mx-3 white rounded z-depth-3">
            {{ layoutContent | safe }}
        </div>
    </div>
</div>


