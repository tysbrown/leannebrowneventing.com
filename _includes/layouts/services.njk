---
layout: layouts/base.njk
section: services
permalink: /services/index.html
---
<header class="pageparallax">
    <div class="container page-header">
        <div class="row align-items-center">
            <div class="col-12 text-center">
                <h1 class="responsive-header greatvibes hero-header white-text wow fadeIn" data-wow-duration="5s" style="line-height:600px;">My <span style="color: #4caf50">Services</span></h1>
            </div>
        </div>
    </div>
</header>
<div class="container my-3">
    <div class="row">
        <div class="col-lg-12">
            <a href="/" class="black-text" style="text-decoration: underline;">Home</a> > Services
        </div>
    </div>
</div>
<div class="container p-xl-0 p-lg-0 p-md-2 p-sm-2 p-2" style="margin-bottom:100px;margin-top:100px">
    <div class="container">
        <div class="row">
            <div class="col-md-4 mt-xl-0 mt-lg-0 mt-md-5 mt-sm-5 mt-5">
                <div class="container-fluid">
                    <div class="row mb-4 justify-content-center">
                        <i class="fas fa-award fa-5x green-text"></i>
                    </div>
                    <div class="row text-center">
                        {{ toprow.textbox1 }}
                    </div>
                </div>
            </div>
            <div class="col-md-4 mt-xl-0 mt-lg-0 mt-md-5 mt-sm-5 mt-5">
                <div class="container-fluid">
                    <div class="row mb-4 justify-content-center">
                        <i class="fas fa-brain fa-5x green-text"></i>
                    </div>
                    <div class="row text-center">
                        {{ toprow.textbox2 }}
                    </div>
                </div>
            </div>
            <div class="col-md-4 mt-xl-0 mt-lg-0 mt-md-5 mt-sm-5 mt-5">
                <div class="container-fluid">
                    <div class="row mb-4 justify-content-center">
                        <i class="fas fa-check fa-5x green-text"></i>
                    </div>
                    <div class="row text-center">
                        {{ toprow.textbox3 }}
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="container-fluid grey lighten-4" style="margin-top:100px;margin-bottom:100px;">
    <div class="container px-xl-0 px-lg-0 px-md-2 px-sm-2 px-2" style="padding-top:100px;padding-bottom:100px;">
        <div class="row">
            <div class="col-lg-12 p-3 d-flex flex-column text-center">
                <h1 class="h1-responsive font-italic">
                    {{ secondrowtext }}
                </h1>
            </div>
        </div>
    </div>
</div>
<div class="container p-xl-0 p-lg-0 p-md-2 p-sm-2 p-2" style="margin-bottom:50px;">
<div class="container-fluid">
    <div class="row">
        <div class="col-lg-6 p-0">
            <img src="{{ training.image }}" width="100%;" class="z-depth-3" id="TRAINING">
        </div>
        <div class="col-lg-6 p-3 d-flex flex-column justify-content-center">
            <h3 class="mb-5 green-text font-weight-bold text-uppercase mt-xl-0 mt-lg-0 mt-md-5 mt-sm-5 mt-5">{{ training.header }}</h3>
            <p>{{ training.body }}</p>
        </div>
    </div>
</div>
</div>
<div class="container-fluid grey lighten-4" style="margin-top:100px;margin-bottom:100px;">
<div class="container px-xl-0 px-lg-0 px-md-2 px-sm-2 px-2" style="padding-top:100px;padding-bottom:100px;">
    <div class="row">
        <div class="col-lg-6 p-3 d-flex flex-column justify-content-center">
            <h3 class="mb-5 green-text font-weight-bold text-uppercase mt-xl-0 mt-lg-0 mt-md-5 mt-sm-5 mt-5">{{ coaching.header }}</h3>
            <p>{{ coaching.body }}</p>
        </div>
        <div class="col-lg-6 p-0">
            <img src="{{ coaching.image }}" width="100%;" class="z-depth-3" id="COACHING">
        </div>
    </div>
</div>
</div>
<div class="container p-xl-0 p-lg-0 p-md-2 p-sm-2 p-2" style="margin-top:100px;margin-bottom:100px;">
<div class="container-fluid">
    <div class="row">
        <div class="col-lg-6 p-0">
            <img src="{{ breaking.image }}" width="100%;" class="z-depth-3" id="BREAKING">
        </div>
        <div class="col-lg-6 p-3 d-flex flex-column justify-content-center">
            <h3 class="mb-5 green-text font-weight-bold text-uppercase mt-xl-0 mt-lg-0 mt-md-5 mt-sm-5 mt-5">{{ breaking.header }}</h3>
            <p>{{ breaking.body }}</p>
        </div>
    </div>
</div>
</div>
