---
layout: layouts/base.njk
section: home
---
<!-- Parallax header -->
<header class="parallax bg">
    <div class="container h-100">
        <div class="row align-items-center" style="height: 90vh;">
            <div class="col-12 text-center">
                <h1 class="responsive-header greatvibes white-text wow fadeIn" data-wow-duration="5s">Leanne Brown <span style="color: #4caf50">Eventing</span></h1>
                <span class="lead text-white font-weight-bold my-3 wow fadeInUp">EQUINE TRAINER AND COACH</span>
            </div>
        </div>
        <div class="row align-items-center" style="height: 10vh;">
            <div class="col-12 text-center text-white">
                <a id="TOP" href="#TOP"><i class="fas fa-4x fa-arrow-alt-circle-down green-text wow bounce" data-wow-iteration="5"></i></a>
            </div>
        </div>
    </div>
</header>
<!-- Parallax header end -->
<!-- ABOUT ME -->
<section class="container wow fadeInUp">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="responsive-header subheadingsize text-center font-weight-bolder greatvibes">Welcome</h1>
        </div>
    </div>
    <div class="row">
        <div class="container-fluid z-depth-3 rounded mx-xl-0 mx-lg-0 mx-md-2 mx-sm-2 mx-2">
            <div class="row">
                <div class="col-lg-6 p-0">
                    <img src="{{ image1 }}" width="100%" class="rounded">
                </div>
                <div class="col-lg-6 d-flex align-items-center mt-xl-0 mt-lg-0 mt-md-5 mt-sm-5 mt-5">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-12 m-xl-3 m-lg-3 m-md-1 m-sm-1 m-1">
                                <h3 class="h3-responsive text-uppercase font-weight-bold green-text text-right">ABOUT ME</h3>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12 m-xl-3 m-lg-3 m-md-1 m-sm-1 m-1">
                                <h1 class="h1-responsive text-uppercase font-weight-bold">Equine trainer and coach in greater Orlando area</h1>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12 m-xl-3 m-lg-3 m-md-1 m-sm-1 m-1">
                                {{ layoutContent | safe }}
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12 m-xl-3 m-lg-3 m-md-1 m-sm-1 m-1">
                                <a class="btn btn-dark-green m-0 mb-3" href="/about/">More Info</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- ABOUT ME END -->
<!-- SERVICES -->
<div class="container-fluid grey lighten-4" style="padding-bottom:100px;margin-top:100px;">
<section class="container wow fadeInUp">
    <div class="row">
        <div class="col-md-12">
            <h1 class="responsive-header subheadingsize text-center font-weight-bolder greatvibes">Services Provided</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-md-4">
            <a href="services.html#TRAINING" class="cardbg1 font-weight-bold white-text d-flex align-items-center justify-content-center m-1" style="height:250px;">TRAINING</a>
        </div>
        <div class="col-md-4">
            <a href="services.html#COACHING" class="cardbg2 font-weight-bold white-text d-flex align-items-center justify-content-center m-1" style="height:250px;">COACHING</a>
        </div>
        <div class="col-md-4">
            <a href="services.html#BREAKING" class="cardbg3 font-weight-bold white-text d-flex align-items-center justify-content-center m-1" style="height:250px;">BREAKING</a>
        </div>
    </div>
</section>
</div>
<!-- SERVICES END -->
<!-- TESTIMONIALS -->
<section class="container p-xl-0 p-lg-0 p-md-2 p-sm-2 p-2 wow fadeInUp">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="responsive-header subheadingsize text-center font-weight-bolder greatvibes">Client Testimonials</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-3 justify-content-xl-center d-none d-lg-flex">
            <i class="fas fa-7x fa-quote-left"></i>
        </div>
        <div class="col-lg-6">
            <div id="testimonialcarousel" class="carousel slide z-depth-3 rounded" data-ride="carousel">
                <div class="carousel-inner">
                    <div class="carousel-item active" data-interval="5000">
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col-md-12 green darken-4 d-flex p-4 align-items-center rounded-top" style="height:130px;">
                                    <i class="fas fa-7x fa-quote-left d-lg-none text-white"></i>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12 text-center">
                                    <img src="{{ testimonials.image1 }}" style="height:150px;width:150px;margin-top:-85px;" class="img-fluid rounded-circle z-depth-3">
                                    <div class="my-5"><h3>{{ testimonials.name1 }}</h3></div>
                                    <div class="font-italic mt-5" style="height:200px;">
                                        {{ testimonials.body1 }}
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="carousel-item" data-interval="5000">
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col-md-12 green darken-4 d-flex p-4 align-items-center rounded-top" style="height:130px;">
                                    <i class="fas fa-7x fa-quote-left d-lg-none text-white"></i>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12 text-center">
                                    <img src="{{ testimonials.image2 }}" style="height:150px;width:150px;margin-top:-85px;" class="img-fluid rounded-circle z-depth-3">
                                    <div class="my-5"><h3>{{ testimonials.name2 }}</h3></div>
                                    <div class="font-italic mt-5" style="height:200px;">
                                        {{ testimonials.body2 }}
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="carousel-item" data-interval="5000">
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col-md-12 green darken-4 d-flex p-4 align-items-center rounded-top" style="height:130px;">
                                    <i class="fas fa-7x fa-quote-left d-lg-none text-white"></i>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-12 text-center">
                                    <img src="{{ testimonials.image3 }}" style="height:150px;width:150px;margin-top:-85px;" class="img-fluid rounded-circle z-depth-3">
                                    <div class="my-5"><h3>{{ testimonials.name3 }}</h3></div>
                                    <div class="font-italic mt-5" style="height:200px;">
                                        {{ testimonials.body3 }}
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-3 justify-content-xl-center d-none d-lg-flex">
            <i class="fas fa-7x fa-quote-right"></i>
        </div>
    </div>
</section>
<!-- TESTIMONIALS END -->
<div class="container-fluid grey lighten-4" style="padding-bottom:100px;padding-top: 100px;margin-top:100px;">
    <section class="container">
        <div class="row">
            <div class="col-12 text-center font-italic">
                <h1 class="h1-responsive">
                    Whether it's coaching, training, or breaking in a young horse, I can work with you to get the results
                    you seek. I work closely with each of my clients to implement a structured plan of action that is driven
                    by results.
                </h1>
            </div>
        </div>
    </section>
</div>
<!-- CONTACT FORM -->
<section class="container wow fadeInUp">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="responsive-header subheadingsize text-center font-weight-bolder greatvibes">Get in Touch</h1>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12 p-4 green darken-4 rounded-top">
            <i class="fas fa-4x fa-envelope text-white"></i>
            <p class="white-text mt-3">
                Interested in hiring me? Enter your info below to set up a free initial consultation with me by email or phone.
            </p>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12 p-4 white z-depth-3 rounded-bottom">
            <form id="contactform" name="contactform" method="POST" data-netlify="true">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="md-form">
                            <input type="text" id="firstname" name="FirstName" class="form-control">
                            <label for="firstname">First Name</label>
                            <small class="firstnamestatus red-text"></small>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="md-form">
                            <input type="text" id="lastname" name="LastName" class="form-control">
                            <label for="lastname">Last Name</label>
                            <small class="lastnamestatus red-text"></small>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6">
                        <div class="md-form">
                            <input type="text" id="email" name="Email" class="form-control">
                            <label for="email">Email Address</label>
                            <small id="emailmessage" class="form-text text-muted">We'll never share your email with anyone else.</small>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="md-form">
                            <input type="text" id="email2" name="Email2" class="form-control">
                            <label for="email2">Re-Enter Email Address</label>
                            <small class="nomatch emailstatus red-text"></small>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="md-form">
                            <input type="text" id="subject" name="Subject" class="form-control">
                            <label for="subject">Subject</label>
                            <small class="subjectstatus red-text"></small>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="md-form">
                            <textarea type="text" id="message" name="Message" rows="10" class="form-control md-textarea"></textarea>
                            <label for="message">Message</label>
                            <small class="messagestatus red-text"></small>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <button type="submit" class="btn btn-green m-0" onclick="validateForm();">Send</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</section>
<!-- CONTACT FORM END -->
